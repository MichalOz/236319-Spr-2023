fun sig1 x y z =  if 1>0 then y else z(x,y);
fun sig2 (x,y) z = if size(z(y/y)^" ")+x < 2 then true else false;
fun sig3 x y z w = x y z;
fun sig4 x y z w = z*w;
fun sig5 x y z = z(x(y), x(y));
fun sig6 () () = 1;
fun sig7 x (y,z) = if 1 > 2 then x else (if 2>3 then y else z);
fun sig8 (x,y,z) = (x div x, y^y, z^z);